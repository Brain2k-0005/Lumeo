@page "/components/chart"

<PageTitle>Chart â€” Lumeo</PageTitle>

<PageHeader Title="Chart" Description="Beautiful charts built on Apache ECharts. Includes 25 convenience components for bar, line, area, pie, donut, radar, scatter, heatmap, treemap, sunburst, funnel, sankey, candlestick, boxplot, graph, tree, theme river, parallel, pictorial bar, waterfall, nightingale, polar bar, effect scatter, and mixed charts." />

<div class="space-y-8">
    <ComponentDemo Title="Bar Chart" Code="@_barCode">
        <div class="w-full">
            <BarChart Categories="_barCategories" Series="_barSeries" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="Line Chart" Code="@_lineCode">
        <div class="w-full">
            <LineChart Categories="_lineCategories" Series="_lineSeries" Smooth="true" ShowDots="true" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="Area Chart" Code="@_areaCode">
        <div class="w-full">
            <AreaChart Categories="_areaCategories" Series="_areaSeries" Smooth="true" Gradient="true" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="Pie Chart" Code="@_pieCode">
        <div class="w-full">
            <PieChart Data="_pieData" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="Donut Chart" Code="@_donutCode">
        <div class="w-full">
            <DonutChart Data="_donutData" CenterLabel="Total" InnerRadius="55%" OuterRadius="75%" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="Radar Chart" Code="@_radarCode">
        <div class="w-full">
            <RadarChart Indicators="_radarIndicators" Series="_radarSeries" FillArea="true" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="Radial / Gauge Chart" Code="@_radialCode">
        <div class="w-full">
            <RadialChart Value="72" Label="Progress" Color="#6366f1" ShowProgress="true" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="Scatter Chart" Code="@_scatterCode">
        <div class="w-full">
            <ScatterChart Series="_scatterSeries" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="Heatmap Chart" Code="@_heatmapCode">
        <div class="w-full">
            <HeatmapChart XCategories="_heatmapX" YCategories="_heatmapY" Data="_heatmapData" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="Treemap Chart" Code="@_treemapCode">
        <div class="w-full">
            <TreemapChart Data="_treemapData" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="Sunburst Chart" Code="@_sunburstCode">
        <div class="w-full">
            <SunburstChart Data="_sunburstData" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="Funnel Chart" Code="@_funnelCode">
        <div class="w-full">
            <FunnelChart Data="_funnelData" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="Sankey Chart" Code="@_sankeyCode">
        <div class="w-full">
            <SankeyChart Nodes="_sankeyNodes" Links="_sankeyLinks" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="Candlestick Chart" Code="@_candlestickCode">
        <div class="w-full">
            <CandlestickChart Categories="_candlestickCategories" Data="_candlestickData" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="BoxPlot Chart" Code="@_boxplotCode">
        <div class="w-full">
            <BoxPlotChart Categories="_boxplotCategories" Data="_boxplotData" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="Graph Chart" Code="@_graphCode">
        <div class="w-full">
            <GraphChart Nodes="_graphNodes" Links="_graphLinks" Categories="_graphCategories" Layout="force" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="Tree Chart" Code="@_treeCode">
        <div class="w-full">
            <TreeChart Data="_treeData" Orient="LR" ShowLabels="true" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="Theme River Chart" Code="@_themeRiverCode">
        <div class="w-full">
            <ThemeRiverChart Data="_themeRiverData" ShowLegend="true" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="Parallel Chart" Code="@_parallelCode">
        <div class="w-full">
            <ParallelChart Dimensions="_parallelDimensions" Series="_parallelSeries" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="Pictorial Bar Chart" Code="@_pictorialBarCode">
        <div class="w-full">
            <PictorialBarChart Categories="_pictorialCategories" Series="_pictorialSeries" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="Waterfall Chart" Code="@_waterfallCode">
        <div class="w-full">
            <WaterfallChart Categories="_waterfallCategories" Values="_waterfallValues" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="Nightingale Chart" Code="@_nightingaleCode">
        <div class="w-full">
            <NightingaleChart Data="_nightingaleData" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="Polar Bar Chart" Code="@_polarBarCode">
        <div class="w-full">
            <PolarBarChart Categories="_polarCategories" Values="_polarValues" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="Effect Scatter Chart" Code="@_effectScatterCode">
        <div class="w-full">
            <EffectScatterChart Series="_effectScatterSeries" />
        </div>
    </ComponentDemo>

    <ComponentDemo Title="Mixed Chart (Bar + Line)" Code="@_mixedCode">
        <div class="w-full">
            <MixedChart Categories="_mixedCategories" Series="_mixedSeries" />
        </div>
    </ComponentDemo>
</div>

